<Window x:Class="Session1.CatalogUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Session1"
        mc:Ignorable="d"
        Title="CatalogUser" Height="450" Width="800">
    <Grid>
        <Label x:Name="UserLabel" Content="Добро пожаловать," HorizontalAlignment="Left" Margin="500,10,0,0" VerticalAlignment="Top" Height="25" Width="126"/>
        <Button x:Name="LogoutUserBTN" Content="Выйти" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="LogoutManagerBTN_Click"/>
       
        <Image x:Name="FirstProductIMG" Source="/img/picture.png" HorizontalAlignment="Left" Height="76" Margin="48,90,0,0" VerticalAlignment="Top" Width="100"/>
        <Image x:Name="SecondProductIMG" Source="/img/picture.png" HorizontalAlignment="Left" Height="77" Margin="48,180,0,0" VerticalAlignment="Top" Width="100"/>
        <Image x:Name="ThirdfProductIMG" Source="/img/picture.png" HorizontalAlignment="Left" Height="77" Margin="48,270,0,0" VerticalAlignment="Top" Width="100"/>
    </Grid>
</Window>
